 .INCLUDE "C:\Users\savol\Documents\CyberIndustries\Progetti\6502-computer\programs\util\lcd.65s"
 
 .START start
 
start:
 LDA #$FF	; Imposta tutti i pin della PORTA a OUTPUT
 STA DDRA
 STA PORTA
 
reset:
 LDA #0		; Carica in A il numero 0
loop_1:
 STA PORTA	; Imposta lo stato di PORTA pari al numero
 INA		; Incrementa A
 CMP #16	; Confronta con 16
 BEQ loop_2 
 JMP loop_1

loop_2:
 DEA
 STA PORTA
 CMP #0
 BEQ loop_1
 JMP loop_2